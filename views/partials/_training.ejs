<% if (training.url.match("http://")) { %>
	<a href="<%= training.url %>" target="_blank">
<% } else { %>
	<a href="http://info.neotechnology.com/<%= training.url %>" target="_blank">
<% } %>

<%= training.city %></a> at <%= training.date %>: 
 with 
<%- [].concat( training.presenter ).map(function(presenter) {
            console.log("presenter",presenter);
        if (presenter.name.indexOf('@') == 0) {
            var contributor = contributors[presenter.name.substring(1)];
	        return "<a target='_blank' href='http://twitter.com/"+contributor.twitter+"'>"+contributor.name+"</a>";
        } else {
            return "<a target='_blank' href='http://twitter.com/"+presenter.twitter+"'>"+presenter.name+"</a>";
        }
	}).join(", ") 
%>
at <%= training.location %>
